# Puerto del API de scraping (por defecto: 3001)
SCRAPING_PORT=3001

# Base de datos (opcional)
# DATABASE_URL=postgresql://user:password@localhost:5432/pcando_scraping

# Redis para cache (opcional)
# REDIS_URL=redis://localhost:6379

