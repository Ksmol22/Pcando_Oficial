{
  "name": "pcando-oficial",
  "version": "1.0.0",
  "description": "PC Building Configuration Tool - Una aplicaci√≥n web para configurar y armar computadoras",
  "type": "module",
  "keywords": [
    "pc-building",
    "computer-configuration",
    "hardware",
    "react",
    "typescript",
    "express",
    "postgresql"
  ],
  "homepage": "https://ksmol22.github.io/Pcando_Oficial",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ksmol22/Pcando_Oficial.git"
  },
  "bugs": {
