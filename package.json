  "scripts": {
    "dev": "tsx watch server/index.ts",
    "dev:simple": "tsx watch server/index-simple.ts",
    "dev:minimal": "tsx watch server/index-minimal.ts",
    "dev:db": "tsx watch server/index-db.ts",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch server/api-only.ts",
    "build": "vite build",
    "build:gh-pages": "vite build --base=/Pcando_Oficial/",
    "preview": "vite preview",
    "deploy": "npm run build:gh-pages && gh-pages -d dist/public",
    "start": "npm run verify && concurrently \"npm run start:frontend\" \"npm run start:backend\" \"npm run start:scraping\"",
    "start:frontend": "vite --port 5173 --host",
    "start:backend": "NODE_ENV=production tsx server/index-db.ts",
    "start:scraping": "cd scraping-system && npm start",
    "dev:all": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\" \"cd scraping-system && npm run dev\"",
    "install:all": "npm install && cd scraping-system && npm install",
    "verify": "node verify-setup.cjs",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:seed": "tsx server/seed.ts",
    "db:studio": "drizzle-kit studio"
  },
